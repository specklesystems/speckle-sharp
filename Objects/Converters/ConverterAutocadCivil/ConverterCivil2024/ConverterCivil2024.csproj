<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>Objects.Converter.Civil3D2024</AssemblyName>
    <RootNamespace>Objects.Converter.Civil</RootNamespace>
    <PackageId>Speckle.Objects.Converter.Civil2024</PackageId>
    <DefineConstants>$(DefineConstants);CIVIL2024</DefineConstants>
    <Description>Converter for Civil3D 2024</Description>
    <PackageTags>$(PackageTags) objects converter civil3d</PackageTags>
    <CopyToKitFolder>true</CopyToKitFolder>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Speckle.AutoCAD.API" Version="2024.0.0" />
    <PackageReference Include="Speckle.Civil3D.API" Version="2024.0.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\..\..\Core\Core\Core.csproj" />
    <ProjectReference Include="..\..\..\Objects\Objects.csproj" />
  </ItemGroup>

  <Import Project="..\ConverterAutocadCivilShared\ConverterAutocadCivilShared.projitems"
      Label="Shared" />
</Project>
