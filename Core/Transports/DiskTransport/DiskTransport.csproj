<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>Speckle.Transports.Disk</PackageId>
    <Product>DiskTransport</Product>
    <Description>A Disk transport for Speckle</Description>
    <PackageTags>$(PackageTags) disk transport</PackageTags>
    <Configurations>Debug;Release</Configurations>
    <Platforms>AnyCPU</Platforms>
    <IsPackable>true</IsPackable>
    <Nullable>enable</Nullable>
    <RootNamespace>Speckle.Core.Transports</RootNamespace>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>TRACE;</DefineConstants>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\Core\Core.csproj" />
  </ItemGroup>
</Project>
